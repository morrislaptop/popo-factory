{
    "name": "morrislaptop/popo-factory",
    "description": "Making it easy to mock your POPO's",
    "keywords": [
        "morrislaptop",
        "popo-factory"
    ],
    "homepage": "https://github.com/morrislaptop/popo-factory",
    "license": "MIT",
    "authors": [
        {
            "name": "Craig Morris",
            "email": "craig.michael.morris@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "anteris-dev/faker-map": "^2.0",
        "fakerphp/faker": "^1.13",
        "symfony/property-access": "^5.2 || ^6.0",
        "symfony/property-info": "^5.2 || ^6.0",
        "symfony/serializer": "^5.2 || ^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "nesbot/carbon": "^2.43",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.16",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Morrislaptop\\PopoFactory\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morrislaptop\\PopoFactory\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
